  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  <!-- ===== (A) BuildInfo.g.cs 생성: 최대한 이르게 생성 ===== -->
  <Target Name="GenerateBuildInfo_Early" BeforeTargets="PrepareForBuild">
    <PropertyGroup>
      <BuildTime>$([System.DateTime]::Now.ToString("yyyy-MM-dd"))</BuildTime>
      <GenDir>$(IntermediateOutputPath)Generated</GenDir>
      <BuildInfoFile>$(GenDir)\BuildInfo.g.cs</BuildInfoFile>
      <BuildInfoNamespace>STK_ToolBox.Common</BuildInfoNamespace>
    </PropertyGroup>
    <MakeDir Directories="$(GenDir)" />
    <ItemGroup>
      <_BuildInfoLines Include="// Generated by MSBuild" />
      <_BuildInfoLines Include="namespace $(BuildInfoNamespace)" />
      <_BuildInfoLines Include="{" />
      <_BuildInfoLines Include="    internal static class BuildInfo" />
      <_BuildInfoLines Include="    {" />
      <!-- 반드시 세미콜론 포함 -->
      <_BuildInfoLines Include="        public const string BuildDateLocal = &quot;$(BuildTime)&quot;%3B" />
      <_BuildInfoLines Include="    }" />
      <_BuildInfoLines Include="}" />
    </ItemGroup>
    <WriteLinesToFile File="$(BuildInfoFile)" Lines="@(_BuildInfoLines)" Overwrite="true" />
  </Target>
  <!-- ===== (B) Version.g.cs 생성: version.txt 값으로 버전 주입 ===== -->
  <PropertyGroup>
    <VersionFile>$(MSBuildProjectDirectory)\version.txt</VersionFile>
    <DefaultVersion>1.1.0.0</DefaultVersion>
  </PropertyGroup>
  <Target Name="GenerateVersionFile_Early" BeforeTargets="PrepareForBuild">
    <ReadLinesFromFile File="$(VersionFile)" ContinueOnError="WarnAndContinue">
      <Output TaskParameter="Lines" ItemName="_VersionLine" />
    </ReadLinesFromFile>
    <PropertyGroup>
      <RawVersion>%(_VersionLine.Identity)</RawVersion>
      <!-- 줄바꿈/공백 제거 -->
      <CurrentVersion>$([System.Text.RegularExpressions.Regex]::Replace('$(RawVersion)','\s',''))</CurrentVersion>
      <CurrentVersion Condition="'$(CurrentVersion)'==''">$(DefaultVersion)</CurrentVersion>
      <GenDir>$(IntermediateOutputPath)Generated</GenDir>
      <VersionFilePath>$(GenDir)\Version.g.cs</VersionFilePath>
    </PropertyGroup>
    <MakeDir Directories="$(GenDir)" />
    <ItemGroup>
      <_VerLines Include="// Generated by MSBuild" />
      <_VerLines Include="[assembly: System.Reflection.AssemblyVersion(&quot;$(CurrentVersion)&quot;)]" />
      <_VerLines Include="[assembly: System.Reflection.AssemblyFileVersion(&quot;$(CurrentVersion)&quot;)]" />
    </ItemGroup>
    <WriteLinesToFile File="$(VersionFilePath)" Lines="@(_VerLines)" Overwrite="true" />
  </Target>
  <!-- ===== (C) 컴파일 포함: CoreCompile 직전에 아이템 추가 ===== -->
  <Target Name="IncludeGeneratedCode_JustInTime" BeforeTargets="CoreCompile">
    <PropertyGroup>
      <GenDir>$(IntermediateOutputPath)Generated</GenDir>
      <BuildInfoFile>$(GenDir)\BuildInfo.g.cs</BuildInfoFile>
      <VersionFilePath>$(GenDir)\Version.g.cs</VersionFilePath>
    </PropertyGroup>
    <ItemGroup>
      <Compile Include="$(BuildInfoFile)" Condition="Exists('$(BuildInfoFile)')">
        <Link>Common\BuildInfo.g.cs</Link>
      </Compile>
      <Compile Include="$(VersionFilePath)" Condition="Exists('$(VersionFilePath)')">
        <Link>Common\Version.g.cs</Link>
      </Compile>
    </ItemGroup>
    <!-- 디버그용 메시지 -->
    <Message Text="Including generated: $(BuildInfoFile)" Importance="High" Condition="Exists('$(BuildInfoFile)')" />
    <Message Text="Including generated: $(VersionFilePath)" Importance="High" Condition="Exists('$(VersionFilePath)')" />
  </Target>
  <!-- ===== (D) 수동 버전 올리기: /t:BumpVersion /p:Part=Build;Step=1 ===== -->
  <UsingTask TaskName="BumpVersionTask" TaskFactory="CodeTaskFactory" AssemblyFile="$(MSBuildToolsPath)\Microsoft.Build.Tasks.v4.0.dll">
    <ParameterGroup>
      <File ParameterType="System.String" Required="true" />
      <Part ParameterType="System.String" Required="false" />
      <Step ParameterType="System.Int32" Required="false" />
      <New Output="true" ParameterType="System.String" />
      <Current Output="true" ParameterType="System.String" />
    </ParameterGroup>
    <Task>
      <Reference Include="System" />
      <Code Type="Fragment" Language="cs"><![CDATA[
        var path = File;
        if (!System.IO.File.Exists(path))
          System.IO.File.WriteAllText(path, "1.1.0.0"+System.Environment.NewLine);
        var text = System.IO.File.ReadAllText(path).Trim();
        Current = text;
        var parts = text.Split('.');
        if (parts.Length != 4) throw new System.Exception("version.txt must be 'a.b.c.d'");
        int major=int.Parse(parts[0]), minor=int.Parse(parts[1]), build=int.Parse(parts[2]), rev=int.Parse(parts[3]);
        var p = (Part ?? "Build").ToLowerInvariant();
        int step = Step==0 ? 1 : Step;
        switch(p){
          case "major": major+=step; minor=0; build=0; rev=0; break;
          case "minor": minor+=step; build=0; rev=0; break;
          case "build": build+=step; rev=0; break;
          case "revision": case "rev": rev+=step; break;
          default: throw new System.Exception("Part must be Major|Minor|Build|Revision");
        }
        New = major+"."+minor+"."+build+"."+rev;
        System.IO.File.WriteAllText(path, New + System.Environment.NewLine);
      ]]></Code>
    </Task>
  </UsingTask>
  <Target Name="BumpVersion">
    <BumpVersionTask File="$(MSBuildProjectDirectory)\version.txt" Part="$(Part)" Step="$(Step)">
      <Output TaskParameter="New" PropertyName="NewVersion" />
      <Output TaskParameter="Current" PropertyName="OldVersion" />
    </BumpVersionTask>
    <Message Text="Version bumped: $(OldVersion) → $(NewVersion) (Part=$(Part), Step=$(Step))" Importance="High" />
  </Target>
